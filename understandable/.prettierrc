{
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "always",
    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
        "^react$",
        "^react-dom$",
        "^next",
        "<THIRD_PARTY_MODULES>",
        "",
        "^@/lib/(.*)$",
        "^@/hooks/(.*)$",
        "^@/components/(.*)$",
        "^@/(.*)$",
        "",
        "^[./]"
    ]
}
